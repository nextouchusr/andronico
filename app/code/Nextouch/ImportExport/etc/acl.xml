<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Nextouch_ImportExport::importexport" title="Nextouch ImportExport" translate="title">
                    <resource id="Nextouch_ImportExport::import" title="Nextouch Import" translate="title">
                        <resource id="Nextouch_ImportExport::run" title="Nextouch Import Run" translate="title"/>
                    </resource>
                </resource>

                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Nextouch_ImportExport::config_import_export" title="ImportExport Section" translate="title"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
